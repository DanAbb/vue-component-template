{
    "extends": ["vue"],
    "plugins": ["vue"],
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "space-before-function-paren": ["error", {
            "anonymous": "always",
            "named": "never"
        }],
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn"
    }
}
